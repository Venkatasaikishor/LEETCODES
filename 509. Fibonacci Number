public class Solution {
    public int fib(int n) {
        if (n == 0) return 0;
        if (n == 1) return 1;
        
        int a = 0, b = 1, c = 0;
        for (int i = 2; i <= n; i++) {
            c = a + b;
            a = b;
            b = c;
        }
        return c;
    }

    public static void main(String[] args) {
        Solution solution = new Solution();
        int n = 5;
        System.out.println("Fibonacci(" + n + ") = " + solution.fib(n));
    }
}
